<?xml version="1.0" encoding="utf-8" ?>
<!--

  Rule definition of standard English grammar for Wintermute.
  This document represents the means of converting linguistics into
  ontological information in the English language. God be with us.

 -->
<RuleSet locale="en">
    <Rule type="Ad" primary="B">
        <Bind with="B,D,E" />
    </Rule>

    <Rule type="Aen">
        <Bind with="C,D" />
    </Rule>

    <Rule type="Aep">
        <Bind with="B,E" />
    </Rule>

    <Rule type="Ax">
        <Bind with="B,Ap" />
    </Rule>

    <Rule type="B">
        <Bind with="B,D,E" />
        <Bind with="C" postAction="reverse" />
    </Rule>

    <Rule type="C" primary="B">
        <Bind with="A,B,C,D,E,F" />
    </Rule>
    <Added />
</RuleSet>
